                <boolProp name="resetInterpreter">false</boolProp>
                <stringProp name="script">import org.apache.jmeter.samplers.SampleResult;

// Получаем HTTP-ответ из предыдущего запроса
SampleResult prev = prev;

if (prev != null) {
    // Извлекаем &quot;_id&quot; из HTTP-ответа
    String id = prev.getResponseDataAsString(); // Предположим, что &quot;_id&quot; находится в теле ответа

    // Получаем номер текущего потока
    int threadNumber = ctx.getThreadNum();

    // Формируем имя переменной для сохранения &quot;_id&quot; (например, id_1, id_2 и так далее)
    String propertyName = &quot;id_&quot; + threadNumber;

    // Сохраняем &quot;_id&quot; в bsh.shared как ассоциативный массив
    bsh.shared.put(propertyName, id);
}
